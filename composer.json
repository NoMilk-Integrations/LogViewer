{
  "name": "nomilk/log-viewer",
  "description": "A Laravel package for viewing and pruning application logs",
  "type": "library",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "pestphp/pest": "^4.0",
    "pestphp/pest-plugin-laravel": "^4.0",
    "orchestra/testbench": "^10.6"
  },
  "autoload": {
    "psr-4": {
      "NoMilk\\LogViewer\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "autoload-dev": {
  },
  "extra": {
    "laravel": {
      "providers": [
        "NoMilk\\LogViewer\\LogViewerServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "./vendor/bin/pest"
  }
}
